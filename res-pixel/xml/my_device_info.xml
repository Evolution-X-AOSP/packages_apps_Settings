<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:settings="http://schemas.android.com/apk/res-auto"
    android:key="my_device_info_pref_screen"
    android:title="@string/about_settings">

    <com.android.settingslib.widget.LayoutPreference
        android:key="my_device_info_header"
        android:layout="@layout/settings_entity_header"
        android:order="0"
        android:selectable="false"
        settings:isPreferenceVisible="false" />

    <PreferenceCategory
        android:key="basic_info_category"
        android:selectable="false"
        android:title="@string/my_device_info_basic_info_category_title">

        <com.android.settings.widget.ValidatedEditTextPreference
            android:key="device_name"
            android:order="1"
            android:summary="@string/summary_placeholder"
            android:title="@string/my_device_info_device_name_preference_title"
            settings:controller="com.android.settings.deviceinfo.DeviceNamePreferenceController"
            settings:enableCopying="true" />

        <Preference
            android:key="branded_account"
            android:order="2"
            android:summary="@string/summary_placeholder"
            android:title="@string/my_device_info_account_preference_title"
            settings:controller="com.android.settings.deviceinfo.BrandedAccountPreferenceController"
            settings:enableCopying="true" />

        <!-- Preference
            android:key="phone_number"
            android:order="3"
            android:selectable="false"
            android:summary="@string/summary_placeholder"
            android:title="@string/status_number"
            settings:controller="com.android.settings.deviceinfo.PhoneNumberPreferenceController"
            settings:enableCopying="true"/ -->

    </PreferenceCategory>

    <PreferenceCategory
        android:title="@string/my_device_info_legal_category_title">

        <Preference
            android:fragment="com.android.settings.LegalSettings"
            android:key="legal_container"
            android:order="15"
            android:title="@string/legal_information" />

        <Preference
            android:key="regulatory_info"
            android:order="16"
            android:title="@string/regulatory_labels">

            <intent
                android:action="android.settings.SHOW_REGULATORY_INFO" />

        </Preference>

        <Preference
            android:key="safety_info"
            android:order="17"
            android:title="@string/safety_and_regulatory_info">

            <intent
                android:action="android.settings.SHOW_SAFETY_AND_REGULATORY_INFO" />

        </Preference>

    </PreferenceCategory>

    <PreferenceCategory
        android:key="device_detail_category"
        android:selectable="false"
        android:title="@string/my_device_info_device_details_category_title">

        <Preference
            android:key="sim_status"
            android:order="18"
            android:summary="@string/summary_placeholder"
            android:title="@string/sim_status_title"
            settings:enableCopying="true"
            settings:keywords="@string/keywords_sim_status" />

        <Preference
            android:fragment="com.android.settings.deviceinfo.hardwareinfo.HardwareInfoFragment"
            android:key="device_model"
            android:order="31"
            android:summary="@string/summary_placeholder"
            android:title="@string/model_info"
            settings:controller="com.android.settings.deviceinfo.HardwareInfoPreferenceController"
            settings:enableCopying="true"
            settings:keywords="@string/keywords_model_and_hardware" />

        <Preference
            android:key="imei_info"
            android:order="32"
            android:summary="@string/summary_placeholder"
            android:title="@string/status_imei"
            settings:controller="com.android.settings.deviceinfo.imei.ImeiInfoPreferenceController"
            settings:enableCopying="true"
            settings:keywords="@string/keywords_imei_info" />

        <Preference
            android:fragment="com.android.settings.deviceinfo.firmwareversion.FirmwareVersionSettings"
            android:key="firmware_version"
            android:order="42"
            android:summary="@string/summary_placeholder"
            android:title="@string/firmware_version"
            settings:controller="com.android.settings.deviceinfo.firmwareversion.FirmwareVersionPreferenceController" />

    </PreferenceCategory>

    <PreferenceCategory
        android:title="@string/my_device_info_device_identifiers_category_title">

        <Preference
            android:key="wifi_ip_address"
            android:order="44"
            android:selectable="false"
            android:summary="@string/summary_placeholder"
            android:title="@string/wifi_ip_address"
            settings:enableCopying="true" />

        <Preference
            android:fragment="com.android.settings.wifi.savedaccesspoints2.SavedAccessPointsWifiSettings2"
            android:key="saved_accesspoints_wifi_mac_address"
            android:order="45"
            android:summary="@string/view_saved_network"
            android:title="@string/status_wifi_mac_address"
            settings:enableCopying="false" />

        <Preference
            android:key="wifi_mac_address"
            android:order="46"
            android:selectable="false"
            android:summary="@string/summary_placeholder"
            android:title="@string/status_device_wifi_mac_address"
            settings:enableCopying="true" />

        <Preference
            android:key="bt_address"
            android:order="47"
            android:selectable="false"
            android:summary="@string/summary_placeholder"
            android:title="@string/status_bt_address"
            settings:enableCopying="true" />

        <Preference
            android:key="up_time"
            android:order="48"
            android:selectable="false"
            android:summary="@string/summary_placeholder"
            android:title="@string/status_up_time" />

        <Preference
            android:key="manual"
            android:order="50"
            android:title="@string/manual">

            <intent
                android:action="android.settings.SHOW_MANUAL" />

        </Preference>

        <Preference
            android:key="device_feedback"
            android:order="51"
            android:title="@string/device_feedback"
            settings:keywords="@string/keywords_device_feedback" />

        <Preference
            android:key="fcc_equipment_id"
            android:order="52"
            android:summary="@string/summary_placeholder"
            android:title="@string/fcc_equipment_id" />

        <Preference
            android:key="build_number"
            android:order="53"
            android:summary="@string/summary_placeholder"
            android:title="@string/build_number"
            settings:controller="com.android.settings.deviceinfo.BuildNumberPreferenceController"
            settings:enableCopying="true" />

    </PreferenceCategory>

</PreferenceScreen>
