<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Whether build status should be shown or not. -->
    <bool name="config_show_build_status">false</bool>

    <!-- Device specific doze package -->
    <string name="config_customDozePackage" translatable="false"></string>

    <!-- Whether to show min/max refresh rate in display settings -->
    <bool name="config_show_refresh_rate_controls">false</bool>
    <bool name="config_supports_dynamic_refresh_rate_controls">true</bool>

    <!-- Fingerprint -->
    <bool name="config_is_front_facing_fps" translatable="false">false</bool>

    <!-- Whether to show TouchGestures preference -->
    <bool name="config_supportsTouchGestures">false</bool>

    <!-- Max network scan search time in seconds -->
    <integer name="config_network_scan_helper_max_search_time_sec">300</integer>

    <!-- Whether to enable Lineage Fast Charge Prefernece -->
    <bool name="config_lineageFastChargeSupported">false</bool>

    <!-- Indicates whether device has a side mounted fingerprint sensor,
         but a dedicated one, i.e. not embedded in the power button. -->
    <bool name="config_is_side_fps" translatable="false">false</bool>

    <!-- Battery health -->
    <string name="config_batteryHealthNode"></string>
    <string name="config_batteryRemainingCapacityNode"></string>
    <string name="config_batteryDesignCapacityNode"></string>

    <!-- Whether to show custom screen resolution settings -->
    <bool name="config_show_custom_screen_resolution_switch">true</bool>
</resources>
