<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Defines the location of the fingerprint sensor on the device
         0 = back
         1 = front
         2 = left side
         3 = right side
    -->
    <integer name="config_fingerprintSensorLocation">0</integer>

    <!-- Device specific doze package -->
    <string name="config_customDozePackage" translatable="false"></string>

    <!-- Whether to show or hide sensitive info (like IMEI and Phone Number) on About phone -->
    <bool name="configShowDeviceSensitiveInfo">false</bool>

    <!-- Screen off FOD -->
    <bool name="config_supportScreenOffFod">false</bool>

    <!-- Fingerprint swipe left and right -->
    <string name="fp_swipe_to_dismiss_notifications_title">Swipe fingerprint left or right to dismiss notifications</string>
    <string name="fp_swipe_to_dismiss_notifications_summary">To dismiss your notifications, swipe left or right on the fingerprint sensor on the back of your phone.</string>

    <!-- Battery Health -->
    <string name="config_batCurCap"></string>
    <string name="config_batDesCap"></string>
    <string name="config_batChargeCycle"></string>
</resources>
