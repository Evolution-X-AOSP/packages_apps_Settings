<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Indicates whether device has a front facing fingerprint sensor (not udfps). -->
    <bool name="config_is_front_facing_fps" translatable="false">false</bool>

    <!-- Settings intelligence suggestions class -->
    <string name="config_settingsintelligence_suggestions_class" translatable="false">
        com.android.settings.intelligence.suggestions.SuggestionService
    </string>

    <!-- Whether SELinux status should be shown or not. -->
    <bool name="config_show_selinux_status">true</bool>

    <!-- Device specific doze package -->
    <string name="config_customDozePackage" translatable="false"></string>

    <!-- Whether the given language capitalizes nouns -->
    <bool name="language_capitalizes_nouns">false</bool>

    <!-- Whether to show min refresh rate in display settings -->
    <bool name="config_show_min_refresh_rate_switch">false</bool>

    <!-- Whether to show min/max refresh rate in display settings -->
    <bool name="config_show_refresh_rate_controls">false</bool>
    <bool name="config_supports_dynamic_refresh_rate_controls">true</bool>

    <!-- Smart Charging -->
    <bool name="config_supportSmartCharging">false</bool>

    <!-- Max network scan search time in seconds -->
    <integer name="config_network_scan_helper_max_search_time_sec">300</integer>

    <!-- Whether build status should be shown or not. -->
    <bool name="config_show_build_status">false</bool>
    <bool name="config_show_build_status_exclusive">false</bool>
    <bool name="config_show_build_status_exclusive_test">false</bool>
    <bool name="config_show_build_status_test">false</bool>
</resources>
